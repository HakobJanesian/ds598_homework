{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfhakobjanesian"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"Azure_Data_Lake_Factory_Historical_Weather_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Azure_Data_Lake_Factory_Historical_Weather'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hakobjanesian.dfs.core.windows.net/"
		},
		"HTTP_air_pollution_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.openweathermap.org/data/2.5/weather"
		},
		"Http_Historical_Wather_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://history.openweathermap.org/"
		},
		"Azure_Data_Lake_Factory_Historical_Weather_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hakobjanesian.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfhakobjanesian_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"Azure_Data_Lake_Factory_Historical_Weather_accountKey": {
						"value": "[parameters('Azure_Data_Lake_Factory_Historical_Weather_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"HTTP_air_pollution_properties_typeProperties_url": {
						"value": "[parameters('HTTP_air_pollution_properties_typeProperties_url')]"
					},
					"Http_Historical_Wather_properties_typeProperties_url": {
						"value": "[parameters('Http_Historical_Wather_properties_typeProperties_url')]"
					},
					"Azure_Data_Lake_Factory_Historical_Weather_properties_typeProperties_url": {
						"value": "[parameters('Azure_Data_Lake_Factory_Historical_Weather_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}